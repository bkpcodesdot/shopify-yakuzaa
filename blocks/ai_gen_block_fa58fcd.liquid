<section class="blog-card">
<div class="ai-symptoms-section-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-symptoms-header-{{ ai_gen_id }}">
    {% if block.settings.title_before != blank %}
     <h2 class="ai-symptoms-title-{{ ai_gen_id }}">
  {{ block.settings.title_before }} <span>{{ block.settings.title_highlight }}</span>

    {% endif %}
    
    {% if block.settings.section_subtitle != blank %}
      <p class="ai-symptoms-subtitle-{{ ai_gen_id }}">{{ block.settings.section_subtitle }}</p>
    {% endif %}
  </div>

  <div class="ai-symptoms-list-{{ ai_gen_id }}">
    {% for i in (1..5) %}
      {% liquid
        assign symptom_enabled_key = 'symptom_' | append: i | append: '_enabled'
        assign symptom_title_key = 'symptom_' | append: i | append: '_title'
        assign symptom_description_key = 'symptom_' | append: i | append: '_description'
        assign symptom_science_text_key = 'symptom_' | append: i | append: '_science_text'
        assign symptom_icon_key = 'symptom_' | append: i | append: '_icon'

        assign symptom_enabled = block.settings[symptom_enabled_key]
        assign symptom_title = block.settings[symptom_title_key]
        assign symptom_description = block.settings[symptom_description_key]
        assign symptom_science_text = block.settings[symptom_science_text_key]
        assign symptom_icon = block.settings[symptom_icon_key]%}

      {% if symptom_enabled %}
        <div class="ai-symptom-box-{{ ai_gen_id }}">
          <div class="ai-symptom-number-{{ ai_gen_id }}">{{ i }}.
          <div class="card-title">
          {% if symptom_title != blank %}
            <h3 class="ai-symptom-title-{{ ai_gen_id }}">{{ symptom_title }}</h3>
          {% endif %}
           </div>
          </div>
      
          
          
          
          <div class="ai-symptom-divider-{{ ai_gen_id }}"></div>
          
          {% if symptom_description != blank %}
            <p class="ai-symptom-description-{{ ai_gen_id }}">{{ symptom_description }}</p>
          {% endif %}
          
          {% if symptom_science_text != blank %}
            <div class="ai-symptom-science-{{ ai_gen_id }}">
              <div class="ai-symptom-science-icon-{{ ai_gen_id }}">
                {% if symptom_icon %}
                  <img
                    src="{{ symptom_icon | image_url: width: 100 }}"
                    alt="Science icon"
                    loading="lazy"
                    width="100"
                    height="100"
                  >
                {% else %}
                  <div class="ai-symptom-science-placeholder-{{ ai_gen_id }}">
                    {{ 'image' | placeholder_svg_tag }}
                  </div>
                {% endif %}
              </div><span>
  <em><strong>Science Says:</strong></em>
  <em> {{ symptom_science_text }}</em>
</span>
            </div>
          {% endif %}
        </div>
      {% endif %}
    {% endfor %}
  </div>

  {% if block.settings.cta_text != blank %}
    <div class="ai-symptoms-cta-{{ ai_gen_id }}">
      <a
        href="{{ block.settings.cta_link }}"
        class="ai-symptoms-button-{{ ai_gen_id }}"
      >
        {{ block.settings.cta_text }}
      </a>
    </div>
  {% endif %}
</div>
</section>

