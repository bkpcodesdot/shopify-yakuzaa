<script>
  (function() {
    class PrimalQueenHero{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentTestimonial = 1;
        this.testimonialCount = 0;
        this.autoSlideInterval = null;
      }

      connectedCallback() {
        this.testimonials = this.querySelectorAll('.ai-primal-queen-hero-testimonial-{{ ai_gen_id }}');
        this.dots = this.querySelectorAll('.ai-primal-queen-hero-dot-{{ ai_gen_id }}');
        this.testimonialCount = this.testimonials.length;

        if (this.testimonialCount > 1) {
          this.setupEventListeners();
          this.startAutoSlide();
        }
      }

      disconnectedCallback() {
        if (this.autoSlideInterval) {
          clearInterval(this.autoSlideInterval);
        }
      }

      setupEventListeners() {
        this.dots.forEach((dot, index) => {
          dot.addEventListener('click', () => {
            this.showTestimonial(index + 1);
            this.resetAutoSlide();
          });
        });
      }

      showTestimonial(index) {
        this.testimonials.forEach((testimonial, i) => {
          testimonial.classList.toggle('active', i === index - 1);
        });

        this.dots.forEach((dot, i) => {
          dot.classList.toggle('active', i === index - 1);
        });

        this.currentTestimonial = index;
      }

      nextTestimonial() {
        const next = this.currentTestimonial >= this.testimonialCount ? 1 : this.currentTestimonial + 1;
        this.showTestimonial(next);
      }

      startAutoSlide() {
        this.autoSlideInterval = setInterval(() => {
          this.nextTestimonial();
        }, {{ block.settings.testimonial_auto_slide_duration | times: 10000 }});
      }

      resetAutoSlide() {
        if (this.autoSlideInterval) {
          clearInterval(this.autoSlideInterval);
          this.startAutoSlide();
        }
      }
    }

    customElements.define('primal-queen-hero-{{ ai_gen_id }}', PrimalQueenHero{{ ai_gen_id }});
  })();
</script>

