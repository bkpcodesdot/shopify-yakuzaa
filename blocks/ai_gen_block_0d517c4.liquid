      font-size: calc({{ block.settings.table_font_size }}px * 0.8);
    }

    .ai-comparison-table__cell-{{ ai_gen_id }} {
      padding: 10px 6px;
    }

    .ai-comparison-table__product-image-{{ ai_gen_id }},
    .ai-comparison-table__product-placeholder-{{ ai_gen_id }} {
      width: 60px;
      height: 60px;
    }

    .ai-comparison-table__product-placeholder-{{ ai_gen_id }} svg {
      width: 36px;
      height: 36px;
    }

    .ai-comparison-table__icon-{{ ai_gen_id }} {
      font-size: 18px;
    }

    .ai-comparison-table__button-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.button_font_size }}px * 0.85);
      padding: calc({{ block.settings.button_padding_vertical }}px * 0.7) calc({{ block.settings.button_padding_horizontal }}px * 0.7);
      width: 100%;
      max-width: 300px;
    }
  }
{% endstyle %}

<div class="ai-comparison-table-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-comparison-table__container-{{ ai_gen_id }}">
    <div class="ai-comparison-table__header-{{ ai_gen_id }}">
      {% if block.settings.heading != blank %}
        <h2 class="ai-comparison-table__heading-{{ ai_gen_id }}">
          How Resilia <span class="ai-comparison-table__heading-span-{{ ai_gen_id }}">Stands Apart</span> from the Rest
        </h2>
      {% endif %}
      
      {% if block.settings.subheading != blank %}
        <h3 class="ai-comparison-table__subheading-{{ ai_gen_id }}">{{ block.settings.subheading }}</h3>
      {% endif %}
      
      {% if block.settings.intro_text != blank %}
        <div class="ai-comparison-table__intro-{{ ai_gen_id }}">{{ block.settings.intro_text }}</div>
      {% endif %}
    </div>

    <div class="ai-comparison-table__table-wrapper-{{ ai_gen_id }}">
      <table class="ai-comparison-table__table-{{ ai_gen_id }}">
        <thead>
          <tr class="ai-comparison-table__header-row-{{ ai_gen_id }}">
            <th class="ai-comparison-table__header-cell-{{ ai_gen_id }}">{{ block.settings.column_1_header }}</th>
            <th class="ai-comparison-table__header-cell-{{ ai_gen_id }}">{{ block.settings.column_2_header }}</th>
            <th class="ai-comparison-table__header-cell-{{ ai_gen_id }}">{{ block.settings.column_3_header }}</th>
          </tr>
        </thead>
        <tbody>
          <tr class="ai-comparison-table__row-{{ ai_gen_id }}">
            <td class="ai-comparison-table__cell-{{ ai_gen_id }}">
              {% if block.settings.product_image %}
                <img
                  src="{{ block.settings.product_image | image_url: width: 200 }}"
                  alt="{{ block.settings.product_image.alt | escape }}"
                  class="ai-comparison-table__product-image-{{ ai_gen_id }}"
                  loading="lazy"
                  width="100"
                  height="100"
                >
              {% else %}
                <div class="ai-comparison-table__product-placeholder-{{ ai_gen_id }}">
                  {{ 'product-1' | placeholder_svg_tag }}
                </div>
              {% endif %}
            </td>
            <td class="ai-comparison-table__cell-{{ ai_gen_id }}">{{ block.settings.product_name }}</td>
            <td class="ai-comparison-table__cell-{{ ai_gen_id }}">{{ block.settings.competitor_name }}</td>
          </tr>
          
          {% for i in (1..8) %}
            {% assign feature_key = 'feature_' | append: i %}
            {% assign product_value_key = 'product_value_' | append: i %}
            {% assign competitor_value_key = 'competitor_value_' | append: i %}
            
            {% assign feature_name = block.settings[feature_key] %}
            {% assign product_value = block.settings[product_value_key] %}
            {% assign competitor_value = block.settings[competitor_value_key] %}
            
            {% if feature_name != blank %}
              <tr class="ai-comparison-table__row-{{ ai_gen_id }}">
                <td class="ai-comparison-table__cell-{{ ai_gen_id }} ai-comparison-table__cell--feature-{{ ai_gen_id }}">{{ feature_name }}</td>
                <td class="ai-comparison-table__cell-{{ ai_gen_id }}">
                  {% if product_value == 'check' %}
                    <span class="ai-comparison-table__icon-{{ ai_gen_id }} ai-comparison-table__icon--check-{{ ai_gen_id }}">✓</span>
                  {% elsif product_value == 'cross' %}
                    <span class="ai-comparison-table__icon-{{ ai_gen_id }} ai-comparison-table__icon--cross-{{ ai_gen_id }}">✗</span>
                  {% else %}
                    {{ product_value }}
                  {% endif %}
                </td>
                <td class="ai-comparison-table__cell-{{ ai_gen_id }}">
                  {% if competitor_value == 'check' %}
                    <span class="ai-comparison-table__icon-{{ ai_gen_id }} ai-comparison-table__icon--check-{{ ai_gen_id }}">✓</span>
                  {% elsif competitor_value == 'cross' %}
                    <span class="ai-comparison-table__icon-{{ ai_gen_id }} ai-comparison-table__icon--cross-{{ ai_gen_id }}">✗</span>
                  {% else %}
                    {{ competitor_value }}
                  {% endif %}
                </td>
              </tr>
            {% endif %}
          {% endfor %}
        </tbody>
      </table>
    </div>

    {% if block.settings.button_text != blank %}
      <div class="ai-comparison-table__button-wrapper-{{ ai_gen_id }}">
        <a href="{{ block.settings.button_link }}" class="ai-comparison-table__button-{{ ai_gen_id }}">
          {{ block.settings.button_text }}
        </a>
      </div>
    {% endif %}
  </div>
</div>

