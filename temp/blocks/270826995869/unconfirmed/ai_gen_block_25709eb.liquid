{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-supplement-hero-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding }}px 20px;
    background-color: {{ block.settings.background_color }};
    position: relative;
    overflow: hidden;
  }

  .ai-supplement-hero-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    top: 10%;
    left: 5%;
    width: 80px;
    height: 80px;
    background: radial-gradient(circle, #FFB6C1 40%, transparent 70%);
    border-radius: 50% 30% 60% 40%;
    opacity: 0.6;
    z-index: 1;
  }

  .ai-supplement-hero-{{ ai_gen_id }}::after {
    content: '';
    position: absolute;
    bottom: 15%;
    right: 8%;
    width: 60px;
    height: 60px;
    background: radial-gradient(circle, #FFFF66 50%, transparent 80%);
    border-radius: 40% 60% 30% 70%;
    opacity: 0.5;
    z-index: 1;
  }

  .ai-supplement-hero-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 40px;
    align-items: center;
    position: relative;
    z-index: 2;
  }

  .ai-supplement-hero-content-{{ ai_gen_id }} {
    z-index: 2;
    position: relative;
  }

  .ai-supplement-hero-warning-{{ ai_gen_id }} {
    color: #411158;
    font-weight: bold;
    font-size: 16px;
    margin-bottom: 20px;
    display: flex;
    align-items: center;
    gap: 8px;
  }

  .ai-supplement-hero-headline-{{ ai_gen_id }} {
    font-family: 'Montserrat', sans-serif;
    font-size: {{ block.settings.headline_size }}px;
    color: #1D0032;
    line-height: 1.4;
    margin-bottom: 30px;
    font-weight: 700;
  }

  .ai-supplement-hero-headline-{{ ai_gen_id }} .highlight {
    font-weight: 900;
    color: {{ block.settings.highlight_color }};
  }

  .ai-supplement-hero-cta-{{ ai_gen_id }} {
    background-color: #00D264;
    color: #FFFFFF;
    font-weight: bold;
    font-size: 16px;
    padding: 16px 24px;
    border-radius: 12px;
    border: none;
    cursor: pointer;
    text-decoration: none;
    display: inline-flex;
    align-items: center;
    gap: 8px;
    margin-bottom: 20px;
    transition: all 0.3s ease;
    box-shadow: 0 4px 12px rgba(0, 210, 100, 0.2);
  }

  .ai-supplement-hero-cta-{{ ai_gen_id }}:hover {
    background-color: #00B555;
    transform: translateY(-2px);
    box-shadow: 0 6px 16px rgba(0, 210, 100, 0.3);
  }

  .ai-supplement-hero-guarantee-{{ ai_gen_id }} {
    font-size: 14px;
    color: #3F3F3F;
    margin-bottom: 30px;
    display: flex;
    flex-wrap: wrap;
    gap: 20px;
  }

  .ai-supplement-hero-testimonial-slider-{{ ai_gen_id }} {
    position: relative;
    background-color: #FFFFFF;
    border-radius: 16px;
    box-shadow: 0 4px 16px rgba(0, 0, 0, 0.1);
    overflow: hidden;
  }

  .ai-supplement-hero-testimonial-track-{{ ai_gen_id }} {
    display: flex;
    transition: transform 0.5s ease;
  }

  .ai-supplement-hero-testimonial-slide-{{ ai_gen_id }} {
    min-width: 100%;
    padding: 20px;
    display: flex;
    gap: 15px;
    align-items: flex-start;
  }

  .ai-supplement-hero-testimonial-avatar-{{ ai_gen_id }} {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    flex-shrink: 0;
    overflow: hidden;
    border: 3px solid #FFB6C1;
  }

  .ai-supplement-hero-testimonial-avatar-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-supplement-hero-testimonial-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background: linear-gradient(45deg, #FFB6C1, #FFC0CB);
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    font-size: 24px;
  }

