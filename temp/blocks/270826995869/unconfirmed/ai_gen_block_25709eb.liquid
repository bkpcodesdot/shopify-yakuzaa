    width: 100px;
    height: 100px;
    opacity: 0.5;
  }

  .ai-supplement-hero-overlay-text-{{ ai_gen_id }} {
    position: absolute;
    top: 20px;
    right: 20px;
    background-color: #FFFF66;
    color: #1D0032;
    font-size: 28px;
    font-weight: 900;
    padding: 10px 18px;
