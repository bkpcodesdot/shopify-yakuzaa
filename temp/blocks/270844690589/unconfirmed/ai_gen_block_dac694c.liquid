{% doc %}
  @prompt
    Create Shopify section with background #FFEEF3; top-left logo "PRIMAL QUEEN" in pink with crown symbol; bold dark purple headline "⚠️ ATTENTION: 82% Of Women Suffer From Imbalanced Hormones & Low Energy"; larger heading with pink #FF6F91 and dark purple text "Here’s Why Beef Organ Superfoods Are The Secret to Balancing Hormones & All Day Energy"; bright green #009933 button with white text "UNLOCK YOUR TRUE POWER" and crown icon; under button two green check marks with text "365-Day Money Back Guarantee" and "Free Shipping +"; right side oval black outline containing red-haired woman image drinking water, yellow pink paint splatter behind oval; inside oval bottom center white uppercase "ONE WEEK"; testimonial box with purple border, circular profile image, left italic review text dark gray, reviewer name "Olivia T." in pink bold with verified checkmark icon; 5 pink stars rating; below testimonial five dots fourth pink others gray; clean, sans-serif font; fully responsive; consistent spacing; subtle yellow pink paint splatter low opacity behind oval
    
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-primal-queen-hero-{{ ai_gen_id }} {
    background-color: #FFEEF3;
    padding: 40px 20px;
    min-height: 100vh;
    display: flex;
    align-items: center;
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
  }

  .ai-primal-queen-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: center;
    width: 100%;
  }

  .ai-primal-queen-content-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 30px;
  }

  .ai-primal-queen-logo-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 8px;
    margin-bottom: 20px;
  }

  .ai-primal-queen-logo-text-{{ ai_gen_id }} {
    font-size: 24px;
    font-weight: 700;
    color: #FF6F91;
    letter-spacing: 1px;
  }

  .ai-primal-queen-crown-icon-{{ ai_gen_id }} {
    width: 28px;
    height: 28px;
    fill: #FF6F91;
  }

  .ai-primal-queen-attention-headline-{{ ai_gen_id }} {
    font-size: 20px;
    font-weight: 700;
    color: #4A0E4E;
    line-height: 1.3;
    margin-bottom: 10px;
  }

  .ai-primal-queen-main-headline-{{ ai_gen_id }} {
    font-size: 36px;
    font-weight: 700;
    line-height: 1.2;
    margin-bottom: 30px;
  }

  .ai-primal-queen-main-headline-{{ ai_gen_id }} .pink-text {
    color: #FF6F91;
  }

  .ai-primal-queen-main-headline-{{ ai_gen_id }} .purple-text {
    color: #4A0E4E;
  }

  .ai-primal-queen-cta-section-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 15px;
  }

  .ai-primal-queen-cta-button-{{ ai_gen_id }} {
    background-color: #009933;
    color: white;
    border: none;
    padding: 18px 32px;
    font-size: 18px;
    font-weight: 700;
    border-radius: 8px;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 10px;
    text-decoration: none;
    transition: background-color 0.3s ease;
    text-transform: uppercase;
    letter-spacing: 0.5px;
  }

  .ai-primal-queen-cta-button-{{ ai_gen_id }}:hover {
    background-color: #007a29;
  }

  .ai-primal-queen-guarantees-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 8px;
  }

  .ai-primal-queen-guarantee-item-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 10px;
    color: #009933;
    font-weight: 600;
    font-size: 14px;
  }

  .ai-primal-queen-checkmark-{{ ai_gen_id }} {
    width: 20px;
    height: 20px;
    fill: #009933;
  }

  .ai-primal-queen-image-section-{{ ai_gen_id }} {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
  }

  .ai-primal-queen-paint-splatter-{{ ai_gen_id }} {
    position: absolute;
    width: 400px;
    height: 400px;
    background: radial-gradient(circle, rgba(255, 239, 91, 0.3) 0%, rgba(255, 111, 145, 0.2) 50%, transparent 70%);
    border-radius: 50%;
    z-index: 1;
  }

  .ai-primal-queen-oval-container-{{ ai_gen_id }} {
    position: relative;
    width: 350px;
    height: 450px;
    border: 3px solid #000000;
    border-radius: 50%;
    overflow: hidden;
    z-index: 2;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    padding: 20px;
  }

  .ai-primal-queen-woman-image-{{ ai_gen_id }} {
    width: 100%;
    height: 70%;
    object-fit: cover;
    border-radius: 50%;
  }

  .ai-primal-queen-woman-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 70%;
    background-color: #f0f0f0;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #666;
  }

  .ai-primal-queen-one-week-text-{{ ai_gen_id }} {
    background-color: white;
    color: #000000;
    font-weight: 700;
    font-size: 16px;
    letter-spacing: 2px;
    padding: 8px 16px;
    border-radius: 20px;
    text-align: center;
    margin-top: 10px;
  }

  .ai-primal-queen-testimonial-{{ ai_gen_id }} {
    background-color: white;
    border: 2px solid #4A0E4E;
    border-radius: 12px;
    padding: 20px;
    margin-top: 30px;
    position: relative;
  }

  .ai-primal-queen-testimonial-content-{{ ai_gen_id }} {
    display: flex;
    gap: 15px;
    align-items: flex-start;
  }

  .ai-primal-queen-profile-image-{{ ai_gen_id }} {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    object-fit: cover;
    flex-shrink: 0;
  }

  .ai-primal-queen-profile-placeholder-{{ ai_gen_id }} {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    background-color: #f0f0f0;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
  }

  .ai-primal-queen-testimonial-text-{{ ai_gen_id }} {
    flex: 1;
  }

  .ai-primal-queen-review-text-{{ ai_gen_id }} {
    font-style: italic;
    color: #666;
    font-size: 14px;
    line-height: 1.4;
    margin-bottom: 10px;
  }

  .ai-primal-queen-reviewer-info-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 8px;
    margin-bottom: 8px;
  }

  .ai-primal-queen-reviewer-name-{{ ai_gen_id }} {
    color: #FF6F91;
    font-weight: 700;
    font-size: 14px;
  }

  .ai-primal-queen-verified-icon-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    fill: #009933;
  }

  .ai-primal-queen-stars-{{ ai_gen_id }} {
    display: flex;
    gap: 2px;
    margin-bottom: 15px;
  }

  .ai-primal-queen-star-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    fill: #FF6F91;
  }

  .ai-primal-queen-dots-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: 8px;
  }

  .ai-primal-queen-dot-{{ ai_gen_id }} {
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background-color: #ccc;
  }

  .ai-primal-queen-dot-{{ ai_gen_id }}.active {
    background-color: #FF6F91;
  }

  @media (max-width: 768px) {
    .ai-primal-queen-container-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 40px;
      text-align: center;
    }

    .ai-primal-queen-main-headline-{{ ai_gen_id }} {
      font-size: 28px;
    }

    .ai-primal-queen-attention-headline-{{ ai_gen_id }} {
      font-size: 18px;
    }

    .ai-primal-queen-oval-container-{{ ai_gen_id }} {
      width: 280px;
      height: 360px;
    }

    .ai-primal-queen-paint-splatter-{{ ai_gen_id }} {
      width: 320px;
      height: 320px;
    }
  }
{% endstyle %}

<div class="ai-primal-queen-hero-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-primal-queen-container-{{ ai_gen_id }}">
    <div class="ai-primal-queen-content-{{ ai_gen_id }}">
      <div class="ai-primal-queen-logo-{{ ai_gen_id }}">
        <svg class="ai-primal-queen-crown-icon-{{ ai_gen_id }}" viewBox="0 0 24 24">
          <path d="M5 16L3 6l5.5 4L12 4l3.5 6L21 6l-2 10H5zm2.7-2h8.6l.9-4.4L14 12l-2-4-2 4-3.2-2.4L7.7 14z"/>
        </svg>
        <span class="ai-primal-queen-logo-text-{{ ai_gen_id }}">{{ block.settings.logo_text }}</span>
      </div>

      <h1 class="ai-primal-queen-attention-headline-{{ ai_gen_id }}">
        {{ block.settings.attention_headline }}
      </h1>

      <h2 class="ai-primal-queen-main-headline-{{ ai_gen_id }}">
        <span class="pink-text">{{ block.settings.main_headline_pink }}</span>
        <span class="purple-text">{{ block.settings.main_headline_purple }}</span>
      </h2>

      <div class="ai-primal-queen-cta-section-{{ ai_gen_id }}">
        <a href="{{ block.settings.cta_link }}" class="ai-primal-queen-cta-button-{{ ai_gen_id }}">
          <svg class="ai-primal-queen-crown-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" width="20" height="20">
            <path d="M5 16L3 6l5.5 4L12 4l3.5 6L21 6l-2 10H5zm2.7-2h8.6l.9-4.4L14 12l-2-4-2 4-3.2-2.4L7.7 14z"/>
          </svg>
          {{ block.settings.cta_text }}
        </a>

        <div class="ai-primal-queen-guarantees-{{ ai_gen_id }}">
          <div class="ai-primal-queen-guarantee-item-{{ ai_gen_id }}">
            <svg class="ai-primal-queen-checkmark-{{ ai_gen_id }}" viewBox="0 0 24 24">
              <path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41L9 16.17z"/>
            </svg>
            <span>{{ block.settings.guarantee_1 }}</span>
          </div>
          <div class="ai-primal-queen-guarantee-item-{{ ai_gen_id }}">
            <svg class="ai-primal-queen-checkmark-{{ ai_gen_id }}" viewBox="0 0 24 24">
              <path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41L9 16.17z"/>
            </svg>
            <span>{{ block.settings.guarantee_2 }}</span>
          </div>
        </div>
      </div>

      <div class="ai-primal-queen-testimonial-{{ ai_gen_id }}">
        <div class="ai-primal-queen-testimonial-content-{{ ai_gen_id }}">
          {% if block.settings.profile_image %}
            <img src="{{ block.settings.profile_image | image_url: width: 100 }}" 
                 alt="Customer profile" 
                 class="ai-primal-queen-profile-image-{{ ai_gen_id }}"
                 width="50" height="50">
          {% else %}
            <div class="ai-primal-queen-profile-placeholder-{{ ai_gen_id }}">
              {{ 'image' | placeholder_svg_tag }}
            </div>
          {% endif %}

          <div class="ai-primal-queen-testimonial-text-{{ ai_gen_id }}">
            <p class="ai-primal-queen-review-text-{{ ai_gen_id }}">{{ block.settings.review_text }}</p>
            
            <div class="ai-primal-queen-reviewer-info-{{ ai_gen_id }}">
              <span class="ai-primal-queen-reviewer-name-{{ ai_gen_id }}">{{ block.settings.reviewer_name }}</span>
              <svg class="ai-primal-queen-verified-icon-{{ ai_gen_id }}" viewBox="0 0 24 24">
                <path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41L9 16.17z"/>
              </svg>
            </div>

            <div class="ai-primal-queen-stars-{{ ai_gen_id }}">
              {% for i in (1..5) %}
                <svg class="ai-primal-queen-star-{{ ai_gen_id }}" viewBox="0 0 24 24">
                  <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                </svg>
              {% endfor %}
            </div>

            <div class="ai-primal-queen-dots-{{ ai_gen_id }}">
              {% for i in (1..5) %}
                <div class="ai-primal-queen-dot-{{ ai_gen_id }} {% if i == 4 %}active{% endif %}"></div>
              {% endfor %}
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="ai-primal-queen-image-section-{{ ai_gen_id }}">
      <div class="ai-primal-queen-paint-splatter-{{ ai_gen_id }}"></div>
      
      <div class="ai-primal-queen-oval-container-{{ ai_gen_id }}">
        {% if block.settings.woman_image %}
          <img src="{{ block.settings.woman_image | image_url: width: 400 }}" 
               alt="Woman drinking water" 
               class="ai-primal-queen-woman-image-{{ ai_gen_id }}"
               width="300" height="300">
        {% else %}
          <div class="ai-primal-queen-woman-placeholder-{{ ai_gen_id }}">
            {{ 'image' | placeholder_svg_tag }}
          </div>
        {% endif %}
        
        <div class="ai-primal-queen-one-week-text-{{ ai_gen_id }}">
          {{ block.settings.one_week_text }}
        </div>
      </div>
    </div>
  </div>
</div>

{% schema %}
{
  "name": "Primal Queen hero",
  "settings": [
    {
      "type": "header",
      "content": "Logo"
    },
    {
      "type": "text",
      "id": "logo_text",
      "label": "Logo text",
      "default": "PRIMAL QUEEN"
    },
    {
      "type": "header",
      "content": "Headlines"
    },
    {
      "type": "text",
      "id": "attention_headline",
      "label": "Attention headline",
      "default": "⚠️ ATTENTION: 82% Of Women Suffer From Imbalanced Hormones & Low Energy"
    },
    {
      "type": "text",
      "id": "main_headline_pink",
      "label": "Main headline (pink part)",
      "default": "Here's Why Beef Organ Superfoods Are The Secret to "
    },
    {
      "type": "text",
      "id": "main_headline_purple",
      "label": "Main headline (purple part)",
      "default": "Balancing Hormones & All Day Energy"
    },
    {
      "type": "header",
      "content": "Call to action"
    },
    {
      "type": "text",
      "id": "cta_text",
      "label": "Button text",
      "default": "UNLOCK YOUR TRUE POWER"
    },
    {
      "type": "url",
      "id": "cta_link",
      "label": "Button link"
    },
    {
      "type": "text",
      "id": "guarantee_1",
      "label": "First guarantee",
      "default": "365-Day Money Back Guarantee"
    },
    {
      "type": "text",
      "id": "guarantee_2",
      "label": "Second guarantee",
      "default": "Free Shipping +"
    },
    {
      "type": "header",
      "content": "Images"
    },
    {
      "type": "image_picker",
      "id": "woman_image",
      "label": "Woman image"
    },
    {
      "type": "text",
      "id": "one_week_text",
      "label": "One week text",
      "default": "ONE WEEK"
    },
    {
      "type": "header",
      "content": "Testimonial"
    },
    {
      "type": "image_picker",
      "id": "profile_image",
      "label": "Profile image"
    },
    {
      "type": "textarea",
      "id": "review_text",
      "label": "Review text",
      "default": "This product completely transformed my energy levels and hormone balance. I feel like a new person!"
    },
    {
      "type": "text",
      "id": "reviewer_name",
      "label": "Reviewer name",
      "default": "Olivia T."
    }
  ],
  "presets": [
    {
      "name": "Primal Queen hero"
    }
  ]
}
{% endschema %}